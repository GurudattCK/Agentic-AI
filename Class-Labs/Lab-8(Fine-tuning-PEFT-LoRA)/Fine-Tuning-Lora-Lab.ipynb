        "from pyboxen import boxen\n",
        "import json\n",
        "import torch\n",
        "from transformers import AutoModelForCausalLM, AutoTokenizer\n",
        "\n",
        "def generate_precise_response(prompt, model, tokenizer):\n",
        "    \"\"\"\n",
        "    Generate a more focused and concise response about specific contract provisions\n",
        "    \"\"\"\n",
        "    inputs = tokenizer(prompt, return_tensors=\"pt\", truncation=True, max_length=2048)\n",
        "    device = next(model.parameters()).device\n",
        "    inputs = inputs.to(device)\n",
        "\n",
        "    with torch.no_grad():\n",
        "        outputs = model.generate(\n",
        "            inputs.input_ids,\n",
        "            max_new_tokens=40,  # Further reduced for even more conciseness\n",
        "            num_return_sequences=1,\n",
        "            temperature=0.2,    # Lowered temperature for more deterministic response\n",
        "            do_sample=True\n",
        "        )\n",
        "\n",
        "    response = tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "\n",
        "    # Improved response extraction\n",
        "    clean_response = response.split(prompt)[-1].strip()\n",
        "\n",
        "    # Ensure the response is concise and direct\n",
        "    if len(clean_response) > 100:\n",
        "        clean_response = clean_response[:100]\n",
        "\n",
        "    return clean_response\n",
        "\n",
        "def find_relevant_clauses(questions, keywords):\n",
        "    \"\"\"\n",
        "    Find clauses relevant to data deletion or termination\n",
        "    \"\"\"\n",
        "    relevant_parts = []\n",
        "    for part in questions:\n",
        "        content = part['content'].lower()\n",
        "        # More precise keyword matching\n",
        "        if any(keyword in content for keyword in keywords):\n",
        "            # Limit clause length to avoid overwhelming the model\n",
        "            relevant_parts.append(content[:300])\n",
        "    return relevant_parts\n",
        "\n",
        "def analyze_data_deletion_provision(questions):\n",
        "    \"\"\"\n",
        "    Specifically analyze data deletion provisions\n",
        "    \"\"\"\n",
        "    deletion_keywords = [\n",
        "        'data deletion',\n",
        "        'customer data',\n",
        "        'terminate',\n",
        "        'termination',\n",
        "        'delete',\n",
        "        'destruction',\n",
        "        'end of agreement'\n",
        "    ]\n",
        "\n",
        "    relevant_parts = find_relevant_clauses(questions, deletion_keywords)\n",
        "\n",
        "    question = \"Does this contract have a provision that mentions customer data will be deleted upon request or after contract termination?\"\n",
        "\n",
        "    prompt = f\"\"\"\n",
        "    Analyze contract excerpts for data deletion provisions:\n",
        "    {' '.join(relevant_parts)}\n",
        "\n",
        "    {question}\n",
        "    Respond with Yes or No, and a very brief explanation.\n",
        "    \"\"\"\n",
        "\n",
        "    print(\"\\n\" + \"=\" * 50)\n",
        "    print(\"Results Before Fine-Tuning\".center(50))\n",
        "    print(\"=\" * 50 + \"\\n\")\n",
        "\n",
        "    # Print question in boxen\n",
        "    print(boxen(\n",
        "        question,\n",
        "        title=\"Question\",\n",
        "        padding=1,\n",
        "        margin=1,\n",
        "        color=\"yellow\"\n",
        "    ))\n",
        "\n",
        "    # Generate and print response in boxen\n",
        "    response = generate_precise_response(prompt, model, tokenizer)\n",
        "    print(boxen(\n",
        "        response,\n",
        "        title=\"Model Response\",\n",
        "        padding=1,\n",
        "        margin=1,\n",
        "        color=\"blue\"\n",
        "    ))\n",
        "\n",
        "    return response\n",
        "\n",
        "# Assume model and tokenizer are already loaded\n",
        "# Main analysis\n",
        "with open(test_file_path, \"r\") as f:\n",
        "    questions = [json.loads(line) for line in f]\n",
        "\n",
        "result = analyze_data_deletion_provision(questions)"
      ]
